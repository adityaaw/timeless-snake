{
  "expo": {
    "name": "Snake",
    "slug": "timeless-snake",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#a3c43d"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.adwardstark.timelesssnake"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#a3c43d"
      },
      "package": "com.adwardstark.timelesssnake"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Pixeled.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "520c4fa5-2845-4001-ba97-361546c63118"
      }
    }
  }
}
